digraph finite_state_machine {
  fontname="Helvetica,Arial,sans-serif"
  node [fontname="Helvetica,Arial,sans-serif"]
  edge [fontname="Helvetica,Arial,sans-serif"]
  rankdir=LR;
  node [shape = circle, style=filled, fillcolor="#dddddd"];

  1  [label="1: StartRow"];
  2  [label="2: InComment"];
  3  [label="3: StartColumn", shape=doublecircle, fillcolor="#ffdddd"];
  4  [label="4: InColumn"];
  5  [label="5: InQuotedColumn"];
  6  [label="6: InEscapedQuote"];
  7  [label="7: InDoubleEscapedQuote"];
  8  [label="8: EndColumnSeparator"];
  9  [label="9: EndColumnRow", shape=doublecircle, fillcolor="#ffdddd"];
  10 [label="10: InRowEnd", shape=doublecircle, fillcolor="#ffdddd"];
  11 [label="11: CRLF"];
  12 [label="12: EndRow"];

  1  -> 1  [label="term / discard"];
  1  -> 2  [label="comment / discard"];
  1  -> 3  [label="* / ε"];

  2  -> 1  [label="LF / discard"];
  2  -> 2  [label="* / discard"];

  3  -> 4  [label="* / copyout"];
  3  -> 5  [label="quote & @quoting / discard"];
  3  -> 8  [label="sep / discard"];
  3  -> 9  [label="term / ε"]

  4  -> 4  [label="* / copyout"];
  4  -> 8  [label="sep / discard"];
  4  -> 9  [label="term / ε"]

  5  -> 5  [label="* / copyout"];
  5  -> 6  [label="esc & @quoting / discard"];
  5  -> 7  [label="quote & @quoting / discard"];

  6  -> 5  [label="* / copyout"];

  7  -> 4  [label="* / copyout"];
  7  -> 5  [label="quote & @quoting & @double-quote / copyout"];
  7  -> 8  [label="sep / discard"];
  7  -> 9  [label="term / ε"]

  8  -> 3  [label="* / ε"];

  9  -> 10 [label="* / ε"];

  10 -> 11 [label="CR & @isCRLF / discard"];
  10 -> 12 [label="* / discard"];

  11 -> 1  [label="* / ε"];
  11 -> 1  [label="LF / discard"];

  12 -> 1  [label="* / ε"];
}
